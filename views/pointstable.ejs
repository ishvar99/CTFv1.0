<% include header%>
<div class="container text-white mb-5">
  <h1 class="float-right" id="levels-text">Points Table</h1>
</div>
<table class="table border-0  text-white" border="1">
    <thead>
      <b>
        <tr>
          <th scope="col">Rank</th>
          <th scope="col">Name</th>
          <th scope="col">Roll No</th>
          <th scope="col">Points</th>
        </tr>
      </b>
    </thead>
    <tbody>

<% users.forEach((user)=>{%>

      <tr>
        <th scope="row">1</th>
        <td><%= user.name%></td>
        <td><%= user.rollno%></td>
        <td><%= user.points%></td>
      </tr>
   <% })%>      
    </tbody>
  </table>